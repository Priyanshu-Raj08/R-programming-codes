vec= c(1,2,3,5,7)
List1= list(1,2,3)
scan(what=numeric())

vec=c(1, 2, 3, 5, 7)
for (i in 1:(length(vec)-1)) {
  for (j in (i+1):length(vec)) {
    if (vec[i] > vec[j]) {
      temp = vec[i]
      vec[i] = vec[j]
      vec[j] = temp
    }
  }
}
