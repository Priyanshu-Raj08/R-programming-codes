a=60
b=100
c=20
if(a>b)
{
  if(a>c)
  {
    print(a)
  }
  }else if(b>a){
    if(b>c){
    print(b)
    }
  }else{
    print(c)
  }

a=10
b=20
c=30
if(a>b && a>c)
{
  print(a)
}else if(b>a && b>c){
  print(b)
}else{
  print(c)
}

